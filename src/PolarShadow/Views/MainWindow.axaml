<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:PolarShadow.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:PolarShadow.Views"
		xmlns:nav="using:PolarShadow.Navigations"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="PolarShadow.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="PolarShadow"
		ExtendClientAreaToDecorationsHint="True"
		ExtendClientAreaChromeHints="PreferSystemChrome"
		ExtendClientAreaTitleBarHeightHint="-1"
		nav:NavigationManager.ContainerName="{x:Static vm:MainWindowViewModel.NavigationName}">
</Window>
