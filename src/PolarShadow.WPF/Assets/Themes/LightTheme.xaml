<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <SolidColorBrush x:Key="PrimaryBackground" Color="White"/>
    <SolidColorBrush x:Key="PrimaryForeground" Color="#242424"/>

    <SolidColorBrush x:Key="SeclondaryBackground" Color="#ebebeb"/>
    <SolidColorBrush x:Key="SeclondaryForeground" Color="#616161"/>
    
    <SolidColorBrush x:Key="AccentBackground" Color="{DynamicResource 110}"/>
    <SolidColorBrush x:Key="AccentForeground" Color="White"/>

    <SolidColorBrush x:Key="PrimaryText" Color="#424242"/>
    <SolidColorBrush x:Key="SecondaryText" Color="#7b8290"/>
    <SolidColorBrush x:Key="AccentText" Color="{DynamicResource 60}"/>

    <SolidColorBrush x:Key="PrimaryBorder" Color="#424242"/>
    <SolidColorBrush x:Key="SecondaryBorder" Color="#bdbdbd"/>
    <SolidColorBrush x:Key="AccentBorder" Color="{DynamicResource 60}"/>

    <SolidColorBrush x:Key="PrimaryLine" Color="#424242"/>
    <SolidColorBrush x:Key="SecondaryLine" Color="#e0e0e0"/>
    <SolidColorBrush x:Key="AccentLine" Color="{DynamicResource 60}"/>

    <SolidColorBrush x:Key="Disable" Color="{DynamicResource 160}"/>

</ResourceDictionary>